{
  "name": "ballcaststats",
  "version": "0.1.0",
  "scripts": {
    "clean": "./node_modules/.bin/bsb -clean-world",
    "build": "./node_modules/.bin/bsb -make-world && ./node_modules/.bin/webpack --config webpack.config.js --config-register @babel/register --env.production",
    "analyze": "./node_modules/.bin/bsb -make-world && ./node_modules/.bin/webpack --config webpack.config.js --config-register @babel/register --env.production --env.analyze",
    "dev": "./node_modules/.bin/webpack-dev-server --host 0.0.0.0 --progress --config-register @babel/register --hot --env.development",
    "bsb": "./node_modules/.bin/bsb -make-world -w"
  },
  "keywords": [
    "BuckleScript"
  ],
  "author": "Ballcast",
  "devDependencies": {
    "@aws-amplify/cli": "^0.1.44",
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@jsiebern/bs-material-ui-ppx": "^1.0.2",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "bs-platform": "^4.0.18",
    "copy-webpack-plugin": "^4.6.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "reductive-dev-tools": "^0.2.6",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.7",
    "terser-webpack-plugin": "^1.2.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.0",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.2.1",
    "webpack-config-utils": "^2.3.1",
    "webpack-dev-server": "^3.1.14"
  },
  "bsc-flags": [
    "-bs-g"
  ],
  "dependencies": {
    "@jsiebern/bs-material-ui": "^1.0.0",
    "@material-ui/core": "^3.8.1",
    "@material-ui/icons": "^3.0.1",
    "aws-amplify": "^1.1.19",
    "aws-sdk": "^2.395.0",
    "babel-polyfill": "^6.26.0",
    "bs-css": "git+https://github.com/ambientlight/bs-css.git",
    "bs-webapi": "^0.13.2",
    "react": "^16.7.0",
    "react-code-input": "^3.8.1",
    "react-dom": "^16.7.0",
    "react-hot-loader": "^4.6.3",
    "reason-react": "^0.5.3",
    "reductive": "git+https://github.com/ambientlight/reductive.git",
    "rxjs": "^6.4.0",
    "whatwg-fetch": "^3.0.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env"
      ],
      [
        "@babel/preset-react"
      ]
    ],
    "plugins": [
      "transform-es2015-modules-commonjs"
    ]
  }
}
