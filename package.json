{
  "name": "ballcaststats",
  "version": "0.1.0",
  "scripts": {
    "clean": "./node_modules/.bin/bsb -clean-world",
    "build": "./node_modules/.bin/bsb -make-world && ./node_modules/.bin/webpack --config webpack.config.js --config-register @babel/register --env.development",
    "dev": "./node_modules/.bin/webpack-dev-server --config-register @babel/register --hot --env.development",
    "bsb": "./node_modules/.bin/bsb -make-world -w"
  },
  "keywords": [
    "BuckleScript"
  ],
  "author": "Ballcast",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "bs-platform": "^4.0.18",
    "copy-webpack-plugin": "^4.6.0",
    "html-webpack-plugin": "^3.2.0",
    "reductive-dev-tools": "git+https://github.com/ambientlight/reductive-dev-tools.git",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.7",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-config-utils": "^2.3.1",
    "webpack-dev-server": "^3.1.14"
  },
  "bsc-flags": [
    "-bs-g"
  ],
  "dependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-hot-loader": "^4.6.3",
    "reason-react": "^0.5.3",
    "reductive": "git+https://github.com/ambientlight/reductive.git"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-es2015-modules-commonjs"
    ]
  }
}
